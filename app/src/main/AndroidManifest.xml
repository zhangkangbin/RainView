<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zkb.rainview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.RedLuckMoneyActivity"
            android:label="@string/title_activity_red_luck_money"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.RainActivity" />
        <activity android:name=".ui.CirleActivity" />
        <activity android:name=".ui.TextViewActivity" />
    </application>

</manifest>